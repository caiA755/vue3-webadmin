{"ast":null,"code":"// import NavMenu from '@/components/nav_menu'\nimport localCache from '@/utils/cache';\nimport { Flag } from '@element-plus/icons-vue';\nimport { getCurrentInstance } from \"vue\";\nexport default {\n  props: {\n    width: String\n  },\n\n  data() {\n    return {\n      iscollapse: false,\n      userMenus: localCache.getCache(\"menuinfo\") //   flodicon:Fold \n\n    };\n  },\n\n  created() {\n    console.log();\n    console.log(localCache.getCache(\"menuinfo\"));\n  },\n\n  methods: {\n    flodevent() {\n      if (this.iscollapse) {\n        this.iscollapse = false;\n        this.$emit('success', \"100px\");\n      } else {\n        this.iscollapse = true;\n      }\n    }\n\n  },\n  emits: [\"success\"],\n\n  setup(props, {\n    emit\n  }) {\n    let instance = getCurrentInstance();\n\n    const select = () => {\n      if (instance.data.iscollapse) {\n        instance.data.iscollapse = false;\n        emit('success', '100px');\n      } else {\n        iscollapse = true;\n      }\n    };\n\n    return {\n      select\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAyCA;AACA,OAAOA,UAAP,MAAuB,eAAvB;AACA,SAASC,IAAT,QAAqB,yBAArB;AACA,SAASC,kBAAT,QAAmC,KAAnC;AAEA,eAAe;EACbC,KAAK,EAAE;IACHC,KAAK,EAAEC;EADJ,CADM;;EAIbC,IAAI,GAAE;IACJ,OAAO;MACLC,UAAU,EAAC,KADN;MAELC,SAAS,EAACR,UAAU,CAACS,QAAX,CAAoB,UAApB,CAFL,CAGP;;IAHO,CAAP;EAKD,CAVY;;EAWZC,OAAO,GAAE;IACRC,OAAO,CAACC,GAAR;IACGD,OAAO,CAACC,GAAR,CAAYZ,UAAU,CAACS,QAAX,CAAoB,UAApB,CAAZ;EACH,CAdW;;EAeZI,OAAO,EAAC;IACHC,SAAS,GAAE;MACP,IAAG,KAAKP,UAAR,EAAmB;QACd,KAAKA,UAAL,GAAgB,KAAhB;QACA,KAAKQ,KAAL,CAAW,SAAX,EAAqB,OAArB;MAEJ,CAJD,MAIK;QACA,KAAKR,UAAL,GAAgB,IAAhB;MAEL;IACJ;;EAVG,CAfI;EA2BZS,KAAK,EAAE,CAAC,SAAD,CA3BK;;EA4BZC,KAAK,CAACd,KAAD,EAAO;IAACe;EAAD,CAAP,EAAe;IACnB,IAAIC,QAAQ,GAACjB,kBAAkB,EAA/B;;IACA,MAAMkB,MAAM,GAAE,MAAM;MACZ,IAAGD,QAAQ,CAACb,IAAT,CAAcC,UAAjB,EAA4B;QACxBY,QAAQ,CAACb,IAAT,CAAcC,UAAd,GAAyB,KAAzB;QACAW,IAAI,CAAC,SAAD,EAAY,OAAZ,CAAJ;MACH,CAHD,MAGK;QACAX,UAAU,GAAC,IAAX;MACL;IACP,CAPD;;