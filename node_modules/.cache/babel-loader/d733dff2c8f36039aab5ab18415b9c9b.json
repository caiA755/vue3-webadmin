{"ast":null,"code":"// import NavMenu from '@/components/nav_menu'\nimport localCache from '@/utils/cache';\nimport { Flag } from '@element-plus/icons-vue';\nimport { getCurrentInstance } from \"vue\";\nimport mapMenusToRouter from '@/utils/map_menus.js';\nimport router from '@/router';\nimport { toRaw } from '@vue/reactivity';\nexport default {\n  props: {\n    width: String\n  },\n\n  data() {\n    return {\n      iscollapse: false,\n      userMenus: localCache.getCache(\"menuinfo\"),\n      isShow: true\n    };\n  },\n\n  created() {\n    let menus = Array.from(toRaw(this.userMenus));\n    let routes = mapMenusToRouter(menus); //  console.log(routes);\n\n    router.addRoute(\"main-name\", routes);\n  },\n\n  methods: {\n    menuItemClick(item) {\n      console.log(item.url);\n      this.$router.push({\n        path: `${item.url}`\n      });\n    }\n\n  },\n  emits: [\"success\"],\n\n  setup(props, {\n    emit\n  }) {\n    let instance = getCurrentInstance();\n\n    const select = () => {\n      if (instance.data.iscollapse) {\n        emit('success', '270px');\n        instance.data.iscollapse = false;\n        instance.data.isShow = true;\n      } else {\n        emit('success', '100px');\n        instance.data.iscollapse = true;\n        instance.data.isShow = false;\n      }\n    };\n\n    return {\n      select\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAyCA;AACA,OAAOA,UAAP,MAAuB,eAAvB;AACA,SAASC,IAAT,QAAqB,yBAArB;AACA,SAASC,kBAAT,QAAmC,KAAnC;AACA,OAAQC,gBAAR,MAA8B,sBAA9B;AACA,OAAOC,MAAP,MAAmB,UAAnB;AACA,SAASC,KAAT,QAAsB,iBAAtB;AAEA,eAAe;EACbC,KAAK,EAAE;IACHC,KAAK,EAAEC;EADJ,CADM;;EAIbC,IAAI,GAAE;IACJ,OAAO;MACLC,UAAU,EAAC,KADN;MAELC,SAAS,EAACX,UAAU,CAACY,QAAX,CAAoB,UAApB,CAFL;MAGLC,MAAM,EAAC;IAHF,CAAP;EAKD,CAVY;;EAWZC,OAAO,GAAE;IACH,IAAIC,KAAK,GAACC,KAAK,CAACC,IAAN,CAAWZ,KAAK,CAAC,KAAKM,SAAN,CAAhB,CAAV;IACA,IAAIO,MAAM,GAACf,gBAAgB,CAACY,KAAD,CAA3B,CAFG,CAGJ;;IACCX,MAAM,CAACe,QAAP,CAAgB,WAAhB,EAA4BD,MAA5B;EACL,CAhBW;;EAiBZE,OAAO,EAAC;IACHC,aAAa,CAACC,