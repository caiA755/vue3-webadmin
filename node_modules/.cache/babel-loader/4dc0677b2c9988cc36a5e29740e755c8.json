{"ast":null,"code":"// import NavMenu from '@/components/nav_menu'\nimport localCache from '@/utils/cache';\nimport { Flag } from '@element-plus/icons-vue';\nimport { getCurrentInstance } from \"vue\";\nimport mapMenusToRouter from '@/utils/map_menus.js';\nimport router from '@/router';\nexport default {\n  props: {\n    width: String\n  },\n\n  data() {\n    return {\n      iscollapse: false,\n      userMenus: localCache.getCache(\"menuinfo\"),\n      isShow: true\n    };\n  },\n\n  created() {\n    //  const routes=mapMenusToRouter(this.userMenus);\n    //  router.addRoute(\"main\")\n    //  console.log(localCache.getCache(\"menuinfo\"));\n    console.log(this.userMenus.targer);\n  },\n\n  methods: {\n    flodevent() {\n      if (this.iscollapse) {\n        this.iscollapse = false;\n        this.$emit('success', \"100px\");\n      } else {\n        this.iscollapse = true;\n      }\n    }\n\n  },\n  emits: [\"success\"],\n\n  setup(props, {\n    emit\n  }) {\n    let instance = getCurrentInstance();\n\n    const select = () => {\n      if (instance.data.iscollapse) {\n        emit('success', '270px');\n        instance.data.iscollapse = false;\n        instance.data.isShow = true;\n      } else {\n        emit('success', '100px');\n        instance.data.iscollapse = true;\n        instance.data.isShow = false;\n      }\n    };\n\n    return {\n      select\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAyCA;AACA,OAAOA,UAAP,MAAuB,eAAvB;AACA,SAASC,IAAT,QAAqB,yBAArB;AACA,SAASC,kBAAT,QAAmC,KAAnC;AACA,OAAQC,gBAAR,MAA8B,sBAA9B;AACA,OAAOC,MAAP,MAAmB,UAAnB;AAEA,eAAe;EACbC,KAAK,EAAE;IACHC,KAAK,EAAEC;EADJ,CADM;;EAIbC,IAAI,GAAE;IACJ,OAAO;MACLC,UAAU,EAAC,KADN;MAELC,SAAS,EAACV,UAAU,CAACW,QAAX,CAAoB,UAApB,CAFL;MAGLC,MAAM,EAAC;IAHF,CAAP;EAKD,CAVY;;EAWZC,OAAO,GAAE;IACN;IACA;IACA;IACIC,OAAO,CAACC,GAAR,CAAY,KAAKL,SAAL,CAAeM,MAA3B;EACN,CAhBW;;EAiBZC,OAAO,EAAC;IACHC,SAAS,GAAE;MACP,IAAG,KAAKT,UAAR,EAAmB;QACd,KAAKA,UAAL,GAAgB,KAAhB;QACA,KAAKU,KAAL,CAAW,SAAX,EAAqB,OAArB;MACJ,CAHD,MA