{"ast":null,"code":"// import HelloWorld from './components/HelloWorld.vue'\n// 按需引用\n// import { ElButton } from 'element-plus'\n// import 'element-plus/theme-chalk/index.css'\n// import 'element-plus/theme-chalk/el-button.css'\nimport { loadingPage } from './service/axios_config';\nexport default {\n  name: 'App',\n  created: function () {\n    let loadingInstance = null;\n    axios.interceptors.request.use(function (config) {\n      //在发送请求之前做些什么\n      loadingInstance = ElLoading.service({\n        fullscreen: true,\n        text: \"你好\"\n      });\n      return config;\n    }, function (error) {\n      //对请求错误做些什么\n      return Promise.reject(error);\n    }); // 声明响应拦截器\n\n    axios.interceptors.response.use(response => {\n      // 隐藏 Loading 效果\n      loadingInstance.close();\n      return response;\n    });\n  },\n  components: {// HelloWorld\n  }\n};","map":{"version":3,"mappings":"AAYA;AACA;AACA;AACA;AACA;AACA,SAAQA,WAAR,QAA2B,wBAA3B;AACA,eAAgB;EACdC,IAAI,EAAE,KADQ;EAEdC,OAAO,EAAC,YAAU;IACd,IAAIC,eAAc,GAAI,IAAtB;IACFC,KAAK,CAACC,YAAN,CAAmBC,OAAnB,CAA2BC,GAA3B,CAA+B,UAASC,MAAT,EAAiB;MAC5C;MACAL,eAAc,GAAIM,SAAS,CAACC,OAAV,CAAkB;QAAEC,UAAU,EAAE,IAAd;QAAoBC,IAAI,EAAE;MAA1B,CAAlB,CAAlB;MACA,OAAOJ,MAAP;IACH,CAJD,EAIG,UAASK,KAAT,EAAgB;MACf;MACA,OAAOC,OAAO,CAACC,MAAR,CAAeF,KAAf,CAAP;IACH,CAPD,EAFgB,CAUhB;;IACAT,KAAK,CAACC,YAAN,CAAmBW,QAAnB,CAA4BT,GAA5B,CAAiCS,QAAD,IAAc;MAC1C;MACAb,eAAe,CAACc,KAAhB;MACA,OAAOD,QAAP;IACH,CAJD;EAKD,CAlBa;EAmBdE,UAAU,EAAE,CACV;EADU;AAnBE,CAAhB","names":["loadingPage","name","created","loadingInstance","axios","interceptors","request","use","config","ElLoading","service","fullscreen","text","error","Promise","reject","response","close","components"],"sourceRoot":"","sources":["E:\\CaiCai\\前端学习\\Vue学习\\job-web-vue3\\src\\App.vue"],"sourcesContent":["<template>\n  <img alt=\"Vue logo\" src=\"./assets/logo.png\">\n  <!-- <HelloWorld msg=\"Welcome to Your Vue.js App\"/> -->\n    <div>\n      <el-button>AAAAAAA</el-button>\n       <el-button type=\"info\">AAAAAAA</el-button>\n    </div>\n  <el-calendar/>\n    <el-link>aaa</el-link>\n</template>\n\n<script>\n// import HelloWorld from './components/HelloWorld.vue'\n// 按需引用\n// import { ElButton } from 'element-plus'\n// import 'element-plus/theme-chalk/index.css'\n// import 'element-plus/theme-chalk/el-button.css'\nimport {loadingPage}  from './service/axios_config'\nexport default  {\n  name: 'App',\n  created:function(){\n      let loadingInstance = null\n    axios.interceptors.request.use(function(config) {\n        //在发送请求之前做些什么\n        loadingInstance = ElLoading.service({ fullscreen: true, text: \"你好\" })\n        return config;\n    }, function(error) {\n        //对请求错误做些什么\n        return Promise.reject(error);\n    });\n    // 声明响应拦截器\n    axios.interceptors.response.use((response) => {\n        // 隐藏 Loading 效果\n        loadingInstance.close()\n        return response\n    });\n  },\n  components: {\n    // HelloWorld\n\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}